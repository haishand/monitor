<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.domain.DeviceData">
    <resultMap id="result" type="domain.DeviceData">
        <result property="code" column="code" />
        <result property="param1" column="param1"/>
        <result property="param2" column="param2"/>
        <result property="recordTime" column="recordTime"/>

    </resultMap>

    <select id="selectAll" resultMap="result">
        select * from device;
    </select>
    <insert id="addOne" parameterType="domain.DeviceData">
        insert into device(
            code,
            param1,
            param2,
            recordTime)
        values(
            #{code},
            #{param1},
            #{param2},
            #{recordTime})
    </insert>
</mapper>